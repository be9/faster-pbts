// A type used in JSDoc comments generated by protobuf.js
JSDocType {
  Type       = NonemptyListOf<SingleType, "|">

  SingleType  = ArrayType | ObjectType | PromiseType | AnyType | Simple
  ArrayType   = "Array.<" Type ">"
  ObjectType  = "Object.<" Type "," Type ">"
  PromiseType = "Promise<" Type ">"
  AnyType     = "*"

  Simple     = id (id | ".")* "="?      -- type
             | stringLiteral            -- literal

  id = alnum | "_" | "$"
  stringLiteral = "\"" id* "\""
}
